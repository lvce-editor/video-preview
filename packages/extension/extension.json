{
  "id": "builtin.video-preview",
  "name": "Video Preview",
  "description": "Video Preview",
  "browser": "src/videoPreviewMain.ts",
  "activation": ["onWebView:builtin.video-preview"],
  "languages": [],
  "webViews": [
    {
      "id": "builtin.video-preview",
      "rpc": true,
      "selector": [".mp4", ".webm", ".ogv"],
      "path": "media/index.html",
      "elements": [
        {
          "type": "title",
          "value": "Video Preview"
        },
        {
          "type": "script",
          "path": "media/index.js"
        },
        {
          "type": "css",
          "path": "media/index.css"
        }
      ]
    }
  ]
}
